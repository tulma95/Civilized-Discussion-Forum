  },
  "homepage": "https://github.com/tulma95/Civilized-Discussion-Forum#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.2",
    "pg": "^7.17.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1"
  }
}